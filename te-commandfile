echo "$SHELL"
export VAULT_URL=http://vault.devops-ontap.com:8200 | sudo tee -a /etc/environment
echo $VAULT_URL
echo "VAULT_URL=http://vault.devops-ontap.com:8200" | sudo tee /etc/profile.d/vault_setup.sh
source /etc/environment
vault login -method=aws header_value=vault.devops-ontap.com role=vault
export TE_GROUP=$(vault kv get -field=token concourse/cisco-fso-labs/te-group) | sudo tee -a /etc/environment
curl -Os https://downloads.thousandeyes.com/agent/install_thousandeyes.sh
chmod a+x install_thousandeyes.sh
./install_thousandeyes.sh -f $TE_GROUP

