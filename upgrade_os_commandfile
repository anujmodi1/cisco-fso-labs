echo "$SHELL"
ps -p $$
cat /etc/shells
sudo apt -y update
sudo apt -y upgrade
#install vault
sudo curl -fsSL https://apt.releases.hashicorp.com/gpg | sudo apt-key add -
sudo apt-add-repository "deb [arch=amd64] https://apt.releases.hashicorp.com $(lsb_release -cs) main"
sudo apt update && sudo apt -y install vault
vault --version
ls -la *.sh
sudo chmod a+x .sh
sudo ./vault.sh