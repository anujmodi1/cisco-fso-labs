export TERMINFO=/usr/lib/terminfo
TERM=xterm
echo "Working Directory is........"
pwd
cd /opt
sudo mkdir appdynamics
cd appdynamics/
sudo mkdir machineagent
sudo chown -R ubuntu:ubuntu /opt/appdynamics
cd machineagent
wget https://download-files.appdynamics.com/download-file/machine-bundle/21.12.6.3236/machineagent-bundle-64bit-linux-22.2.0.3282.zip
unzip machineagent-bundle-64bit-linux-22.2.0.3282.zip
nohup /opt/appdynamics/machineagent/jre/bin/java -Dappdynamics.controller.hostName=cisco-apipartnertraininglab.saas.appdynamics.com
-Dappdynamics.controller.port=443 -Dappdynamics.controller.ssl.enabled=true -Dappdynamics.agent.accountAccessKey=x6r3ex9h5t4l
-Dappdynamics.agent.accountName=cisco-apipartnertraininglab
-Dappdynamics.sim.enabled=true -Dappdynamics.agent.maxMetrics=300000 -jar machineagent.jar &

